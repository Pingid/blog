{
  "name": "my-blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.danbeaven.co.uk/blog",
  "dependencies": {
    "gh-pages": "^1.0.0",
    "markdown": "^0.5.0",
    "marked": "^0.3.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^2.5.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
